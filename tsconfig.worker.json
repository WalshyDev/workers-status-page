{
	"compilerOptions": {
		"target": "esnext",
		"lib": ["ESNext"],
		"module": "esnext",
		"skipLibCheck": true,
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"composite": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"types": ["./worker-configuration.d.ts"]
	},
	"include": ["src/**/*"]
}
